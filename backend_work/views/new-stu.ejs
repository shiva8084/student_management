<html>
    <head>
        <title>
            <%=title%>
        </title>
        <link rel="stylesheet" href="../css/new-stu.css">
    </head>
    <body>
        <a href="/backtoadminlogin">BACK</a>
        <form action="btoe" method="post">
            <p>STUDENT_ID</p><input type="text" name="stuid" required>
            <p>NAME</p><input type="text" name="nam" placeholder="aman" required>
            <p>DATE OF BIRTH</p><input type="date" name="date" placeholder="10/10/2000" required>
            <p>GENDER</p><input type="text" name="gender" maxlength="1" placeholder="m" required>
            <p>ADDRESS</p><input type="text" name="address" placeholder="address" required>
            <p>FATHER NAME</p><input type="text" name="fnam" placeholder="rahul" required>
            <p>MOTHER NAME</p><input type="text" name="mnam" placeholder="anjali" required>
            <p>MOBILE NO</p><input pattern="\d*" name="num"  placeholder="124569874" maxlength="10" required>
            <p>EMAIL</p><input type="email" name="email" placeholder="admin76@gmail.com" required>
            <p>CLASS TEACHER</p><input type="text" name="ctnam" placeholder="ashok" required>
            <button type="submit">SUBMIT</button>
        </form>
    </body>
</html>